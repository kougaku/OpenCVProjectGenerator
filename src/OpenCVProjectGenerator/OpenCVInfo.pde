
class OpenCVInfo {
  String version_label;
  String version_number;
  String base_file_dir;
  String key_file;

  OpenCVInfo(String _version_label, String _version_number, String _base_file_dir, String _key_file ) {
    version_label  = _version_label;
    version_number = _version_number;
    base_file_dir  = _base_file_dir;
    key_file       = _key_file;
  }
}
